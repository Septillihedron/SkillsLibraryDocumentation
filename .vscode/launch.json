    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "localRoot": "${workspaceFolder}\\compiler",
            "program": "${workspaceFolder}\\compiler\\out\\index.js",
            "outFiles": [
                "${workspaceFolder}/compiler/out/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "AutoDoc-simple",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "localRoot": "${workspaceFolder}\\AutoDoc-simple",
            "program": "${workspaceFolder}\\AutoDoc-simple\\autodoc.mjs"
        },
    ]
}
